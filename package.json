{
  "name": "conversion-engine-solid",
  "version": "2.0.0",
  "description": "Music notation conversion engine refactored with SOLID principles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects=\"Unit Tests\"",
    "test:integration": "jest --selectProjects=\"Integration Tests\"",
    "test:performance": "jest --selectProjects=\"Performance Tests\" --runInBand",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "pretest": "npm run build",
    "example": "ts-node src/examples/solid-architecture-example.ts",
    "benchmark": "npm run test:performance",
    "docs": "typedoc src --out docs/api",
    "validate": "npm run lint && npm run format:check && npm run test:ci"
  },
  "keywords": [
    "music",
    "notation",
    "conversion",
    "chord",
    "nashville",
    "solid",
    "design-patterns",
    "typescript"
  ],
  "author": "SOLID Refactoring Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-next": "^15.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "crypto": "^1.0.1",
    "next": "^15.4.4",
    "next-pwa": "^5.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "webpack-obfuscator": "^3.5.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/conversion-engine-solid.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/conversion-engine-solid/issues"
  },
  "homepage": "https://github.com/your-org/conversion-engine-solid#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "MIGRATION_GUIDE.md"
  ]
}
